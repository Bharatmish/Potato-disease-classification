{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "main-tf-serving.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/main",
      "dest": "main.py"
    },
    {
      "src": "/api/tf-serving",
      "dest": "main-tf-serving.py"
    }
  ]
}
